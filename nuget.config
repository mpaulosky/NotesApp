<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<clear />
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
		<add key="github-mpaulosky" value="https://nuget.pkg.github.com/mpaulosky/index.json" protocolVersion="3" />
	</packageSources>
	<packageSourceMapping>
		<!-- All packages from nuget.org -->
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
		<!-- Specific packages from GitHub (if any custom packages exist) -->
		<packageSource key="github-mpaulosky">
			<package pattern="mpaulosky.*" />
		</packageSource>
	</packageSourceMapping>
</configuration>
